<?php
function fibonacci($n) {
    if($n==1 || $n==2) {
        return 1;
    }else{
        return fibonacci($n-1) + fibonacci($n-2);
    }
}
for($x=1;$x<=10;$x++) {
    if($x==1) {
        echo '0,';
    }
    if($x!=10) {
        echo fibonacci($x) . ',';
    } else {
        echo fibonacci($x);
    }
}
?>
