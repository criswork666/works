<?php
function fibonacci($n) {
    if($n==1 || $n==2) {
        return 1;
    }else{
        return fibonacci($n-1) + fibonacci($n-2);
    }
}
for($i=1;$i<=10;$i++) {
    if($i==1) {
        echo '0,';
    }
    if($i!=10) {
        echo fibonacci($i) . ',';
    } else {
        echo fibonacci($i);
    }
}
?>
